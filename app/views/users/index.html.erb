<h1>List or users</h1>
<ul>
    <%= @users.each do | user |%>
        <li>
            <div>
                <p>
                    <h2> <%= user.name %></h2>
                </p>
                <div>
                    <%= user.email %>
                </div>
            </div>
        </li>
    <% end %>
</ul>